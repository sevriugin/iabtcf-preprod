{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "moduleResolution": "node",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "preserveConstEnums": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "es2016",
      "DOM"
    ]
  }
}
